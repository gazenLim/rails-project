    <%= button_to 'Google', '/auth/google_oauth2' %>

<%= render partial: '/errors/errors', locals: {obj: @user }%>

<h1>Sign in form</h1>
<%= form_with  model: @user, local: true do |t| %>
<%= t.label :name %>
<%= t.text_field :name %><br>
<%= t.label :username %>
<%= t.text_field :username %><br>
<%= t.label :email %>
<%= t.text_field :email %><br>
<%= t.label :password %>
<%= t.password_field :password %><br>
<%= t.submit "Signup" %>
<% end %>
