

<div>
<% if !logged_in? %>
<ul>
    <%= link_to "Sign Up", new_user_path  %>   | 
    <%= link_to "Log in", login_path %>

<% else %>
<li style="float:right" ><%= button_to 'Logout', logout_path %></li>
</ul>
<% end %>

<% if logged_in? %>
<h3>Login with <%= current_user.user_name%></h3>
<% end %>
</div>
